    <script src="@Url.Content("~/Scripts/jqBootstrapValidation.js")"></script>
    <script src="@Url.Content("~/Scripts/contact_me.js")"></script>

    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/Scripts/freelancer.js")"></script>

    <!-- Custom Javascript -->
    <script src="@Url.Content("~/Scripts/challenge.js")"></script>

    @RenderSection("scripts", required: false)
